    "version": "0.2.0",
    "configurations": [
        {
            "name": "LBF",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceRoot}/lbf/target/debug/lbf.exe",
            "args": [
                // "-i", "${workspaceRoot}/assets/dxf/albano_dxf.json",
                "-i",
                "${workspaceRoot}/assets/albano.json",
                "-c",
                "${workspaceRoot}/assets/config_lbf.json",
                "-s",
                "${workspaceRoot}/solutions"
            ],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "preLaunchTask": "cargo build"
        },
        {
            "name": "SVG Collision",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceRoot}/svg_collision/target/debug/svg_collision.exe",
            "args": [],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "preLaunchTask": "cargo build"
        }
    ]
}
